#!/usr/bin/env bats

@test "gometalinter installed" {
  run gometalinter

  [ "$status" -eq 0 ]
}

@test "btm installed" {
  run btm --version

  [ "$status" -eq 0 ]
}

@test "aws-google-auth installed" {
  run aws-google-auth --help

  [ "$status" -eq 0 ]
}

@test "php installed" {
  run php -r "echo \"echo\";"

  [ "$status" -eq 0 ]
}
